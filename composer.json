{
    "name":              "go1/rest",
    "license":           "MIT",
    "prefer-stable":     true,
    "minimum-stability": "dev",
    "autoload":          {
        "psr-4": {
            "go1\\rest\\": "./src"
        },
        "files": [
            "./src/helper.php"
        ]
    },
    "require":           {
        "php":                           "^7.4",
        "ext-json":                      "*",
        "beberlei/assert":               "^3.2.0",
        "firebase/php-jwt":              "^5.0",
        "nyholm/psr7":                   "^1.3",
        "nyholm/psr7-server":            "^1.0",
        "php-di/slim-bridge":            "^3.0",
        "php-http/message":              "^1.8.0",
        "psr/http-client":               "^1.0.1",
        "psr/simple-cache":              "^1.0.1",
        "slim/slim":                     "^4.5",
        "symfony/cache":                 "^v5.1.0",
        "symfony/console":               "^v5.1.0",
        "symfony/http-client":           "^v5.1.0",
        "symfony/http-client-contracts": "^v2.1.0",
        "symfony/var-dumper":            "^v5.1.0",
        "symfony/yaml":                  "^v5.1.0",
        "datadog/dd-trace":              "^0.47.0"
    },
    "require-dev":       {
        "doctrine/dbal":               "^2.9.2",
        "elasticsearch/elasticsearch": "^5.4.0",
        "phpunit/phpunit":             "^9.0",
        "justinrainbow/json-schema":   "^5.2.10"
    },
    "suggest":           {
        "php-amqplib/php-amqplib": "^2.6.0"
    },
    "bin":               ["rest-cli.php"],
    "extra":             {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    }
}
